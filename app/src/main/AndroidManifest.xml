<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.diction">

    <application
        android:allowBackup="true"
        android:icon="@drawable/windmill"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Numbers"
            android:label="@string/category_numbers"/>
        <activity android:name=".Months"
            android:label="@string/category_months"/>
        <activity android:name=".Colors"
            android:label="@string/category_colors"/>
        <activity android:name=".Phrases"
            android:label="@string/category_phrases"/>
        <activity android:name=".Family"
            android:label="@string/category_family"/>
        <activity android:name=".Vocabulary"
            android:label="@string/category_vocabulary"/>.
        <activity android:name=".Grammar"
            android:label="@string/category_grammar"/>
    </application>

</manifest>
